{
    "manifest_version": 3,
    "name": "Workspace Lock",
    "version": "1.0",
    "description": "Lock individual Edge workspaces with a password.",
    "permissions": ["tabs", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
}